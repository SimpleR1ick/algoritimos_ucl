   "source": [
    "import numpy as np\n",
    "from numpy.linalg import norm\n",
    "\n",
    "A = np.array([1, 2, 3, 4, 5])\n",
    "B = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "print(\"A: \", A)\n",
    "print(\"B: \", B)\n",
    "\n",
    "cosseno = np.dot(A, B) / (norm(A) * norm(B))\n",
    "print(\"Cos similaridade: \", round(cosseno, 2))"
   ]
  }
